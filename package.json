{
  "name": "vscode-transform-selection",
  "publisher": "unexge",
  "displayName": "Transform Selection",
  "description": "Transform selections with given JavaScript expression.",
  "repository": {
    "url": "https://github.com/unexge/vscode-transform-selection.git",
    "type": "git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:transformSelection.transform"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "transformSelection.transform",
        "title": "Transform Selection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^10.2.0",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
